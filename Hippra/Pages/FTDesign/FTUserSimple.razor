<div class="card" >
    <div class="card-body">
        <div class="d-flex align-items-center">
            <div class="flex-grow-1">
                @name
            </div>
            <div>
                <a class="btn btn-light" href="/FTProfile/@id">Profile</a>
            </div>
        </div>
    </div>
</div>

@code {
    [Parameter]
    public string name { get;set; }
    [Parameter]
    public string id { get; set; }
}
